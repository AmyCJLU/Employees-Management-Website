 <div class="row">     
    <div class="col-md-12">         
        <h1>{{title}}<a href="/employees/add" class="btn btn-success pull-right" style="margin-top:5px;">Add New Employee</a></h1>         
        <hr />     
    </div> 
</div> 
<div class="row">     
    <div class="col-md-12">
        <div class="table-responsive">
            <table class="table">
                <thead>
                <tr>
                    <th>Employee Num</th>
                    <th>Full Name</th>
                    <th>Email</th>
                    <th>Address</th>
                    <th>Manager ID</th>
                    <th>Status</th>
                    <th>Department</th>
                    <th>Hired On</th>
                </tr>
                </thead>
                <tbody>
                {{#each data}}    
                <tr>
                    <td>
                        {{!-- {{#each data}}              --}}
                            {{employeeNum}}<br />         
                        {{!-- {{/each}}   --}}
                    </td>
                    <td>
                        {{!-- {{#each data}}              --}}
                            <a href="/employee/{{employeeNum}}">{{firstName}} {{last_name}}</a><br />         
                        {{!-- {{/each}}   --}}
                    </td>
                    <td>
                        {{!-- {{#each data}}              --}}
                            <a href="mailto:{{email}}">{{email}}</a><br />         
                        {{!-- {{/each}}   --}}
                    </td>
                    <td>
                        {{!-- {{#each data}}              --}}
                            {{addressStreet}} {{addresCity}} {{addressState}} {{addressPostal}}<br />         
                        {{!-- {{/each}}   --}}
                    </td>
                    <td>
                        {{!-- {{#each data}}              --}}
                            <a href="/employees?manager={{employeeManagerNum}}">{{employeeManagerNum}}</a><br />         
                        {{!-- {{/each}}   --}}
                    </td>
                    <td>
                        {{!-- {{#each data}}              --}}
                            <a href="/employees?status={{status}}">{{status}}</a><br />         
                        {{!-- {{/each}}   --}}
                    </td>
                    <td>
                        {{!-- {{#each data}}              --}}
                            <a href="/employees?department={{department}}">{{department}}</a><br />         
                        {{!-- {{/each}}   --}}
                    </td>
                    <td>
                        {{!-- {{#each data}}              --}}
                            {{hireDate}}<br />         
                        {{!-- {{/each}}   --}}
                    </td>
                </tr>
                {{/each}} 
                </tbody>
            </table>
        </div>
     
   
    </div> 
</div> 